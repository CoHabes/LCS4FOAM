FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    //-----------------------------------------------------------------------//
    {
        name    backFringe;
        type    cellSet;
        action  new;
        source  cellToCell;
        set     background;
    }

    {
        name    backFringe;
        type    cellSet;
        action  subset;
        source  boxToCell;
        box (-9 -9 -0.5) (26 9 0.5);
    }

    {
        name    backHole;
        type    cellSet;
        action  new;
        source  cellToCell;
        set     background;
    }

    {
        name    backHole;
        type    cellSet;
        action  subset;
        source  boxToCell;
        box (-8 -8 -0.5) (25 8 0.5);
    }

    {
        name    backFringe;
        type    cellSet;
        action  delete;
        source  cellToCell;
        set     backHole;
    }

    //-----------------------------------------------------------------------//
    {
        name    refineFringe;
        type    cellSet;
        action  new;
        source  cellToCell;
        set     refine;
    }

    {
        name    refineFringe;
        type    cellSet;
        action  subset;
        source  sphereToCell;
        origin  (0 0 0);
        radius  5.5;
    }

    {
        name    refineHole;
        type    cellSet;
        action  new;
        source  cellToCell;
        set     refine;
    }

    {
        name    refineHole;
        type    cellSet;
        action  subset;
        source  sphereToCell;
        origin  (0 0 0);
        radius  5.0;
    }

    {
        name    refineFringe;
        type    cellSet;
        action  delete;
        source  cellToCell;
        set     refineHole;
    }

    //-----------------------------------------------------------------------//

    {
        name    empty;
        type    cellSet;
        action  new;
        source  labelToCell;
        value   ();
    }
);


