FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
     volScalarFieldValue cellDist 0
);

regions
(
    boxToCell
    {
        box (-20 -20 -0.5) (5 20 0.5);

        fieldValues
        (
            volScalarFieldValue cellDist 1
        );
    }
);


